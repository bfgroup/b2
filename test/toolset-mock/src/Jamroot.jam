import print ;
import feature ;

rule write-target-os ( target : : properties * )
{
    local target-os = [ feature.defaults <target-os> ] ;
    print.output $(target) ;
    print.text $(target-os:G=) : true ;
}

make target-os.txt : : @write-target-os ;
